<Window x:Class="Pms.Main.FrontEnd.Wpf.SelectDownloadOption"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pms.Main.FrontEnd.Wpf"
        mc:Ignorable="d"
        Title="Select Download Options" Height="150" Width="225">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <!--<RowDefinition/>-->
        </Grid.RowDefinitions>

        <Button x:Name="BtnDownloadAll" Grid.Row="0" Content="Download All" Click="BtnDownloadAll_Click"/>
        <Button x:Name="BtnDownloadUnconfirmed" Grid.Row="1" Content="Download Unconfirmed" Click="BtnDownloadUnconfirmed_Click"/>
    </Grid>
</Window>
